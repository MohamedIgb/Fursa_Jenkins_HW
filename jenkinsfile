pipeline {
    agent any

    stages {
        stage("create package") {
            steps{
                sh 'mvn clean package'
            }
        }

        stage("save artifact"){
            steps {
                 archiveArtifacts artifacts: 'target/*.jar'
            }
        }
            stage("slack"){
                steps {
                    slackSend channel: 'channel 1', message: 'Hello channel 1'
                }
            }
        
    }

}